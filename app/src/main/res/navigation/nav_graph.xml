<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/searchFragment">

    <include app:graph="@navigation/media_details_nav_graph" />
    <include app:graph="@navigation/search_nav_graph" />
    <include app:graph="@navigation/show_episodes_nav_graph" />
    <include app:graph="@navigation/poster_nav_graph" />

    <fragment
        android:id="@+id/searchFragment"
        android:name="com.hfad.search.SearchFragment"
        android:label="fragment_search"
        tools:layout="@layout/fragment_search">
        <deepLink
            android:id="@+id/deepLinksearchFragment"
            app:uri="android-app://com.hfad.search/searchFragment" />
        <action
            android:id="@+id/navigateSearchToMediaDetailsWithId"
            app:destination="@id/mediaDetailsFragment">
            <argument
                android:name="id"
                app:argType="string"
                android:defaultValue="1"/>
        </action>

    </fragment>

    <fragment
        android:id="@+id/mediaDetailsFragment"
        android:name="com.hfad.media_details.MediaDetailsFragment"
        android:label="fragment_media_details"
        tools:layout="@layout/fragment_media_details">
        <deepLink
            android:id="@+id/deepLinkmediaDetailsFragment"
            app:uri="android-app://com.hfad.mmedia_details/mediaDetailsFragment" />
        <action
            android:id="@+id/navigateMediaDetailsToPosterWithId"
            app:destination="@id/posterFragment">
            <argument
                android:name="id"
                app:argType="string"
                android:defaultValue="1"/>
        </action>
        <action
            android:id="@+id/navigateMediaDetailsToShowEpisodesWithId"
            app:destination="@id/showEpisodesFragment">
            <argument
                android:name="id"
                app:argType="string"
                android:defaultValue="1"/>
        </action>
        <action
            android:id="@+id/navigateMediaDetailsToSearchWithId"
            app:destination="@id/searchFragment">
            <argument
                android:name="id"
                app:argType="string"
                android:defaultValue="1"/>
        </action>
    </fragment>

    <fragment
        android:id="@+id/posterFragment"
        android:name="com.hfad.poster.PosterFragment"
        android:label="fragment_poster"
        tools:layout="@layout/fragment_poster">

        <deepLink
            android:id="@+id/deepLinkPosterFragment"
            app:uri="android-app://com.hfad.poster/PosterFragment" />
        <action
            android:id="@+id/navigatePosterToMediaDetailsWithId"
            app:destination="@id/mediaDetailsFragment">
            <argument
                android:name="id"
                app:argType="string"
                android:defaultValue="1"/>
        </action>
    </fragment>
    <fragment
        android:id="@+id/showEpisodesFragment"
        android:name="com.hfad.show_episodes.ShowEpisodesFragment"
        android:label="fragment_show_episodes"
        tools:layout="@layout/fragment_show_episodes" >
    <deepLink
        android:id="@+id/deepLinkShowEpisodesFragment"
        app:uri="android-app://com.hfad.show_episodes/ShowEpisodesFragment" />
        <action
            android:id="@+id/navigateShowEpisodesToSearchWithId"
            app:destination="@id/searchFragment">
            <argument
                android:name="id"
                app:argType="string"
                android:defaultValue="1"/>
        </action>
        <action
            android:id="@+id/navigateShowEpisodesToMediaDetailsWithId"
            app:popUpTo="@+id/searchFragment"
            app:destination="@id/mediaDetailsFragment">
            <argument
                android:name="id"
                app:argType="string"
                android:defaultValue="1"/>
        </action>
    </fragment>
</navigation>









